{
    "manifest_version": 2,
    "name": "Ninja Coins",
    "version": "1.1",
    "description": "Change the Sant√©dollars icon to Ninja Coins",
    "content_scripts": [
        {
            "js": ["dist/common.js"],
            "matches": ["*://www.etreensante.ca/*"]
        },
        {
            "js": ["dist/heading.js"],
            "matches": ["*://www.etreensante.ca/*/remboursement/demande", "*://www.etreensante.ca/*/bilan/novodollars"]
